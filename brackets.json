{
    "//comment1": "CUSTOM BRACKETS PREFERENCES--------------------------------",
    "fonts.fontSize": "23px",
    "fonts.fontFamily": "Ubuntu Mono",
    "spaceUnits": 2,
    "tabSize": 4,
    "showCursorWhenSelecting": true,
    "styleActiveLine": true,
    "dragDropText": true,
    "insertHintOnTab": true,
    "highlightMatches": true,
    "scrollPastEnd": true,
    "indentLineComment": true,
    "linting.enabled": false,
    "extensions.sort": "downloadCount",
    "//comment2": "CUSTOM EXTENSIONS PREFERENCES------------------------------",
    "themes.theme": "aquaman",
    "brackets-git.dateMode": 3,
    "brackets-git.dateFormat": "YYYY-MM-DD  HH:mm:ss",
    "brackets-git.enableAdvancedFeatures": true,
    "brackets-git.panelShortcut": "Ctrl-Shift-G",
    "brackets-git.commitAllShortcut": "Ctrl-Shift-C",
    "brackets-git.pushShortcut": "Ctrl-Shift-P",
    "brackets-git.showBashButton": false,
    "brackets-git.showTerminalIcon": false,
    "brackets-git.useCodeInspection": false,
    "brackets-git.gitTimeout": 60,
    "hirse.outline.enabled": true,
    "brackets.linkfile.droparea": false,
    "ovk.simple-todo.text-file-storage-todo-file-path": "{PROJECT}/todo.txt",
    "ovk.simple-todo.text-file-storage-todo-category": "# ",
    "ovk.simple-todo.text-file-storage-todo-complete": "[v] ",
    "ovk.simple-todo.text-file-storage-todo-incomplete": "[ ] ",
    "ovk.simple-todo.todoProvider": "text-file-storage",
    "ovk.simple-todo.deleteCompleted": false,
    "brackets-ruler.guideEnabled": true,
    "//comment3": "AUTO-GENERATED BRACKETS PREFERENCES------------------------",
    "brackets-eslint.gutterMarks": true,
    "brackets-eslint.useLocalESLint": false,
    "//comment4": "INDEPENDENT AUTO-GENERATED EXTENSTIONS PREFERENCES---------",
    "soimon.DocumentOutliner.enabled": false,
    "ovk.simple-todo.enabled": false,
    "ovk.simple-todo.trello-storage-key": "",
    "ovk.simple-todo.trello-storage-token": "",
    "ovk.simple-todo.trello-storage-boardId": "",
    "ovk.simple-todo.trello-storage-listId": "",
    "ovk.simple-todo.togglePanelHotkey": "Ctrl-Alt-Shift-T",
    "ovk.simple-todo.addTodoHotkey": "Ctrl-Alt-Shift-A",
    "mikaeljorhult.bracketsTodo.enabled": false,
    "malas34.brackets-consoleplus.opened": false,
    "mikaeljorhult.bracketsTodo.userSettings": {
        "regex": {
            "prefix": "(?:<!--|\\/\\*|\\/\\/|#|--) *@?(",
            "suffix": "):? *(.*?) ?(?=\\/\\*|-->|\\*\\/|\\r\\n|\\n|\\r|$)"
        },
        "tags": [
            "TODO",
            "NOTE",
            "FIXME",
            "CHANGES",
            "FUTURE"
        ],
        "case": false,
        "search": {
            "scope": "current",
            "excludeFolders": [],
            "excludeFiles": []
        },
        "sort": {
            "done": true
        },
        "hide": {
            "done": true
        }
    },
    "//comment5": "DEPENDENT AUTO-GENERATED EXTENSTIONS PREFERENCES-----------",
    "//comment6": "DEPENDENT CUSTOM BRACKETS PREFERENCES-----------",
    "extensions.default.disabled": [
        "L:/Brackets/www/extensions/default/brackets-eslint",
        "L:/Brackets/www/extensions/default/JSLint",
        "L:/Brackets/www/extensions/default/LESSSupport"
    ],
    "//comment7": "TEMPORARY-GENERATED EXTENSTIONS PREFERENCES----------------",
    "//comment8": "NEWLY-GENERATED EXTENSTIONS PREFERENCES--------------------"
}
